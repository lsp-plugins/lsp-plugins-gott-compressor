<plugin resizable="true">
	<vbox spacing="4">
		<grid rows="2" cols="6" vspacing="4" hspacing="4" expand="true">

			<group ui:inject="GraphGroup" ipadding="0" expand="true" fill="true" text="labels.graphs.spectrum">
				<graph width.min="440" height.min="160" expand="true" fill="true">
					<origin hpos="-1" vpos="-1" visible="false"/>

					<!-- Additional grid -->
					<ui:with color="graph_alt_1">
						<ui:for id="f" first="20" last="90" step="10">
							<marker v="${:f}"/>
							<marker v="${:f*10}"/>
							<marker v="${:f*100}"/>
						</ui:for>
						<marker v="20000"/>
						<ui:with ox="1" oy="0">
							<marker v="+12 db"/>
							<marker v="-12 db"/>
							<marker v="-36 db"/>
							<marker v="-60 db"/>

							<ui:for id="g" first="-42" last="6" step="12">
								<marker v="${g} db" visibility=":zoom le -9 db"/>
							</ui:for>
							<ui:for id="g" first="-21" last="3" step="6">
								<marker v="${g} db" visibility=":zoom le -15 db"/>
							</ui:for>
						</ui:with>
					</ui:with>

					<!-- Secondary grid -->
					<ui:with color="graph_sec">
						<marker v="100"/>
						<marker v="1000"/>
						<marker v="10000"/>
						<marker value="0 db" ox="1" oy="0"/>
						<marker value="-24 db" ox="1" oy="0"/>
						<marker value="-48 db" ox="1" oy="0"/>
					</ui:with>

					<!-- Band split markers -->
					<ui:with
						width="2"
						hwidth="2"
						fill="0.5"
						color.a="0.2"
						hcolor.a="0.1"
						lcolor.a="0.8"
						hlcolor.a="0.6"
						rcolor.a="0.8"
						hrcolor.a="0.6"
						color="graph_marker_cycle"
						hcolor="graph_marker_cycle"
						lcolor="graph_marker_cycle"
						hlcolor="graph_marker_cycle"
						rcolor="graph_marker_cycle"
						hrcolor="graph_marker_cycle">

						<marker v="10" color.hue="0" hcolor.hue="0" rborder="16" hrborder="16" rcolor.hue="0" hrcolor.hue="0"/>
						<marker id="sf1" editable="true" pointer="hsize" color.hue="0" hcolor.hue="0" lborder="16" hlborder="16" lcolor.hue="0" hlcolor.hue="0" rborder="16" hrborder="16" rcolor.hue="0.25" hrcolor.hue="0.25"/>
						<marker id="sf2" editable="true" pointer="hsize" color.hue="0.25" hcolor.hue="0.25" lborder="16" hlborder="16" lcolor.hue="0.25" hlcolor.hue="0.25" rborder="16" hrborder="16" rcolor.hue="0.5" hrcolor.hue="0.5"/>
						<marker id="sf3" editable="true" pointer="hsize" color.hue="0.5" hcolor.hue="0.5" lborder="16" hlborder="16" lcolor.hue="0.5" hlcolor.hue="0.5" rborder="16" hrborder="16" rcolor.hue="0.75" hrcolor.hue="0.75" visibility=":ebe"/>
						<marker v="24000" color.hue="(:ebe) ? 0.75 : 0.5" hcolor.hue="(:ebe) ? 0.75 : 0.5" lborder="16" hlborder="16" lcolor.hue="(:ebe) ? 0.75 : 0.5" hlcolor.hue="(:ebe) ? 0.75 : 0.5"/>
					</ui:with>

					<!-- Meshes -->
					<ui:for id="i" first="1" count="4">
						<mesh
							id="bfc_${i}m"
							width="2"
							fill="false"
							smooth="true"
							color="graph_mesh_cycle"
							color.hue="${(:i-1) * 0.25}"
							fcolor="graph_mesh_cycle"
							fcolor.hue="${(:i-1) * 0.25}"
							color.a="0.6"
							fcolor.a="0.9"
							visibility="${(:i lt 4) ? ':flt' : ':flt and :ebe'}"/>
					</ui:for>
					<mesh id="ifg_m" width="0" color="mid_in" color.a="0.8" fcolor.a="0.8" fill="true" fcolor="mid" visibility=":ife_m"/>
					<mesh id="ifg_s" width="0" color="side_in" color.a="0.8" fcolor.a="0.8" fill="true" fcolor="side" visibility=":ife_s"/>
					<mesh id="ofg_m" width="2" color="mid" visibility=":ofe_m"/>
					<mesh id="ofg_s" width="2" color="side" visibility=":ofe_s"/>

					<mesh id="ag_m" width="3" color="mid" smooth="true"/>
					<mesh id="ag_s" width="3" color="side" smooth="true"/>

					<!-- Threshold segments -->
					<ui:for id="b" first="1" count="4">
						<ui:for id="i" first="0" count="2">
							<ui:eval id="color" value=":i ? 'side' : 'mid'"/>

							<ui:set id="ch" value="_${b}${:i ? 's' : 'm'}"/>
							<ui:if test=":b ieq 1">
								<!-- Band 1 -->
								<ui:eval id="sf" value=":i ? 'sqrt (:sf1 * 10)' : '10'"/>
								<ui:eval id="ef" value=":i ? ':sf1' : 'sqrt (:sf1 * 10)'"/>
								<ui:set id="vis" value="true"/>
							</ui:if>
							<ui:if test=":b ieq 2">
								<!-- Band 2 -->
								<ui:eval id="sf" value=":i ? 'sqrt (:sf2 * :sf1)' : ':sf1'"/>
								<ui:eval id="ef" value=":i ? ':sf2' : 'sqrt (:sf2 * :sf1)'"/>
								<ui:set id="vis" value="true"/>
							</ui:if>
							<ui:if test=":b ieq 3">
								<!-- Band 3 -->
								<ui:eval id="sf" value=":i ? 'sqrt ((:ebe ? :sf3 : 24000) * :sf2)' : ':sf2'"/>
								<ui:eval id="ef" value=":i ? '(:ebe ? :sf3 : 24000)' : 'sqrt ((:ebe ? :sf3 : 24000) * :sf2)'"/>
								<ui:set id="vis" value="true"/>
							</ui:if>
							<ui:if test=":b ieq 4">
								<!-- Band 4 -->
								<ui:eval id="sf" value=":i ? 'sqrt (24000 * :sf3)' : ':sf3'"/>
								<ui:eval id="ef" value=":i ? '24000' : 'sqrt (24000 * :sf3)'"/>
								<ui:set id="vis" value=":ebe"/>
							</ui:if>

							<!-- Downward Threshold -->
							<line
								sx="${sf}"
								sy=":td${ch}"
								x="${ef}"
								y.id="td${ch}"
								y.editable="true"
								z.id="rd${ch}"
								z.editable="true"
								width="2"
								smooth="true"
								color="${color}"
								hcolor="${color}"
								lborder="(2 - lg :rd${ch}) * 48"
								hlborder="(2 - lg :rd${ch}) * 48"
								lcolor="${color}"
								lcolor.a="0.6"
								hlcolor.a="0.5"
								hlcolor="${color}"
								pointer="vsize"
								visibility="${vis}"/>

							<!-- Upward Threshold -->
							<line
								sx="${sf}"
								sy=":tu${ch}"
								x="${ef}"
								y.id="tu${ch}"
								y.editable="true"
								z.id="ru${ch}"
								z.editable="true"
								width="2"
								smooth="true"
								color="${color}"
								hcolor="${color}"
								rborder="(2 - lg :ru${ch}) * 48"
								hrborder="(2 - lg :ru${ch}) * 48"
								rcolor="${color}"
								rcolor.a="0.6"
								hrcolor.a="0.5"
								hrcolor="${color}"
								pointer="vsize"
								visibility="${vis}"/>

							<!-- Bottom Threshold -->
							<line sx="${sf}" sy=":tm${ch}" x="${ef}" y.id="tm${ch}" y.editable="true" z.id="kn${ch}" z.editable="true" width="2" smooth="true" color="${color}" hcolor="${color}" pointer="vsize" visibility="${vis}"/>
						</ui:for>
					</ui:for>

					<!-- Boundaries -->
					<marker v="1" ox="2" oy="0" color="graph_prim" visibility="false"/>
					<marker v="24000" color="graph_prim" visibility="false"/>

					<!-- Axis -->
					<axis min="10" max="24000" angle="0.0" log="true" visibility="false"/>
					<axis min="-72 db / (:zoom ** 3)" max="24 db * :zoom" color="graph_prim" angle="0.5" log="true"/>
					<axis min="-1" max="1" visibility="false" angle="0.5"/>

					<!-- Text -->
					<ui:with ox="0" oy="2" y="0" halign="1" valign="1">
						<text x="10" text="graph.units.hz"/>
						<text x="20" text="20"/>
						<text x="30" text="30"/>
						<text x="50" text="50"/>
						<text x="100" text="100"/>
						<text x="200" text="200"/>
						<text x="300" text="300"/>
						<text x="500" text="500"/>
						<text x="1000" text="graph.values.x_k" text:value="1"/>
						<text x="2000" text="graph.values.x_k" text:value="2"/>
						<text x="3000" text="graph.values.x_k" text:value="3"/>
						<text x="5000" text="graph.values.x_k" text:value="5"/>
						<text x="10000" text="graph.values.x_k" text:value="10"/>
						<text x="20000" text="graph.values.x_k" text:value="20"/>
					</ui:with>

					<ui:with x="10" halign="1" valign="1">
						<ui:for id="g" first="-60" last="0" step="12">
							<text y="${g} db" text="${g}"/>
						</ui:for>
						<text y="0 db" ox="0" oy="2" text="graph.units.db" valign="-1"/>
						<ui:for id="g" first="-42" last="6" step="12">
							<text y="${g} db" text="${g}" visibility=":zoom le -9 db"/>
						</ui:for>
						<ui:for id="g" first="-21" last="3" step="6">
							<text y="${g} db" text="${g}" visibility=":zoom le -15 db"/>
						</ui:for>
					</ui:with>
				</graph>
			</group>
			<!-- zoom -->
			<cell rows="2">
				<vbox vexpand="true" bg.color="bg_schema">
					<label font.name="lsp-icons" font.size="12" font.antialias="true" text="E" color="text_icon" pad.b="4" pad.t="4"/>
					<fader id="zoom" angle="1" vexpand="true" bg.inherit="true" flat="true"/>
					<value id="zoom" width="40"/>
				</vbox>
			</cell>

			<!-- Analysis -->
			<cell rows="2">
				<group text="groups.analysis" expand="false" ipadding="0">
					<vbox>
						<grid rows="4" cols="2">
							<align halign="-1" pad.t="4" pad.l="6">
								<label text="labels.fft.fft_in"/>
							</align>
							<hbox spacing="4" pad.v="4" pad.h="6" hfill="false"><button id="ife_m" text="labels.chan.M_id" size="19" ui:inject="Button_mid_8"/>
								<button id="ife_s" text="labels.chan.S_ide" size="19" ui:inject="Button_side_8"/></hbox>

							<cell cols="2"><hsep pad.v="2" bg.color="bg" vreduce="true"/></cell>

							<align halign="-1" pad.t="4" pad.l="6">
								<label text="labels.fft.fft_out" halign="0"/>
							</align>
							<hbox spacing="4" pad.v="4" pad.h="6" hfill="false"><button id="ofe_m" text="labels.chan.M_id" size="19" ui:inject="Button_mid_8"/>
								<button id="ofe_s" text="labels.chan.S_ide" size="19" ui:inject="Button_side_8"/></hbox>

							<cell cols="2"><hsep pad.v="2" bg.color="bg" vreduce="true"/></cell>
						</grid>

						<grid rows="2" cols="2" transpose="false" vexpand="false" height.min="100" hspacing="4">
							<knob id="react" size="20" pad.r="4" pad.l="6"/>
							<vbox vfill="false" pad.r="6">
								<label text="labels.metering.reactivity" halign="0"/>
								<value id="react" width.min="50" sline="true" halign="0"/>
							</vbox>

							<knob id="shift" size="20" pad.r="4" pad.l="6"/>
							<vbox vfill="false" pad.r="6">
								<label text="labels.metering.shift" halign="0"/>
								<value id="shift" width.min="50" sline="true" halign="0"/>
							</vbox>
						</grid>
						<hsep pad.v="2" bg.color="bg" vreduce="true"/>
						<void vexpand="true"/>
					</vbox>
				</group>
			</cell>

			<!-- meter in -->
			<cell rows="2">
				<vbox vexpand="true" bg.color="bg_schema">
					<label text="labels.chan.in" pad.b="3" pad.t="3"/>
					<ledmeter vexpand="true" angle="1">
						<ledchannel id="ilm_l" min="-72 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="left_in"/>
						<ledchannel id="ilm_r" min="-72 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="right_in"/>
					</ledmeter>
				</vbox>
			</cell>

			<!-- meter out -->
			<cell rows="2">
				<vbox vexpand="true" bg.color="bg_schema">
					<label text="labels.chan.out" pad.b="3" pad.t="3"/>
					<ledmeter vexpand="true" angle="1">
						<ledchannel id="olm_l" min="-72 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="left"/>
						<ledchannel id="olm_r" min="-72 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="right"/>
					</ledmeter>
				</vbox>
			</cell>

			<!-- signal -->
			<cell rows="2">
				<vbox>
					<group text="groups.signal" height.min="182" vexpand="false">
						<grid rows="4" cols="2" hspacing="4">
							<knob id="g_in" pad.r="4"/>
							<vbox vfill="false">
								<label text="labels.input" halign="0"/>
								<value id="g_in" sline="true" halign="0"/>
							</vbox>

							<knob id="g_out" pad.r="4"/>
							<vbox vfill="false">
								<label text="labels.output" expand="true" halign="0"/>
								<value id="g_out" width.min="50" sline="true" halign="0"/>
							</vbox>

							<knob id="g_dry" scolor="dry" pad.r="4"/>
							<vbox vfill="false">
								<label text="labels.signal.dry" halign="0"/>
								<value id="g_dry" width.min="50" sline="true" halign="0"/>
							</vbox>

							<knob id="g_wet" scolor="wet" pad.r="4"/>
							<vbox vfill="false">
								<label text="labels.signal.wet" halign="0"/>
								<value id="g_wet" width.min="50" sline="true" halign="0"/>
							</vbox>
						</grid>
					</group>
					<hsep pad.v="2" bg.color="bg" vreduce="true"/>
					<void vexpand="true" bg.color="bg_schema"/>
				</vbox>
			</cell>

			<!-- bottom line -->
			<align halign="-1" vreduce="true">
				<hbox spacing="4" vreduce="true" bg.color="bg_schema" pad.h="6">
					<label text="labels.mode:"/>
					<combo id="mode" pad.r="8" pad.v="4"/>
					<label text="labels.sc_boost:"/>
					<combo id="envb" pad.r="8"/>
					<button id="flt" text="labels.filters" size="22" ui:inject="Button_cyan"/>
				</hbox>
			</align>
		</grid>

		<!-- down part -->
		<grid rows="1" cols="3">
			<!-- Sidechain part -->
			<group text="labels.sidechain" expand="false" vreduce="true" ipadding="0">
				<grid rows="16" cols="1" spacing="0" bg.color="bg_schema">
					<label text="labels.setup" pad.h="6" pad.v="4" vreduce="true"/>
					<vbox vreduce="true">
						<combo id="sc_mode" pad.h="6" pad.b="4"/>
						<combo id="sc_src" pad.h="6" pad.b="4"/>
						<button id="sc_ext" text="labels.chan.external_sc" fill="true" visibility="ex :sc_ext" pad.h="6" pad.b="4"/>
					</vbox>
					<hsep pad.v="2" bg.color="bg" vreduce="true"/>

					<label text="labels.sc.preamp" pad.h="6" pad.v="4" vreduce="true"/>
					<knob id="sc_pamp" scolor="kscale"/>
					<value id="sc_pamp" sline="true" pad.b="4"/>
					<hsep pad.v="2" bg.color="bg" vreduce="true"/>

					<label text="labels.sc.reactivity" pad.h="6" pad.v="4" vreduce="true"/>
					<knob id="sc_rea" scolor="kscale"/>
					<value id="sc_rea" sline="true" pad.b="4"/>
					<hsep pad.v="2" bg.color="bg" vreduce="true"/>

					<label text="labels.sc.lookahead" pad.h="6" pad.v="4" vreduce="true"/>
					<knob id="lkahead"/>
					<value id="lkahead" sline="true" pad.b="4"/>

					<hsep pad.v="2" bg.color="bg" vreduce="true"/>
					<void vexpand="true"/>
				</grid>

			</group>
			<vsep pad.h="2" bg.color="bg" hreduce="true"/>

			<!-- Mid/Side part -->
			<cell>
				<cgroup id="csel" hexpand="true">
					<ui:for id="c" first="0" count="2">
						<ui:eval id="channel" value="(:c ieq 0) ? 'num_id_M_id' : 'num_id_S_ide'"/>

						<grid rows="13" cols="12">
							<!-- header -->

							<!-- Row 1 -->
							<label text="labels.band" pad.v="4"/>
							<cell rows="13">
								<vsep pad.h="2" bg.color="bg" hreduce="true"/>
							</cell>

							<void/>

							<cell rows="13">
								<vsep pad.h="2" bg.color="bg" hreduce="true"/>
							</cell>

							<label text="labels.upward" pad.v="4"/>
							<cell rows="13">
								<vsep pad.h="2" bg.color="bg" hreduce="true"/>
							</cell>
							<label text="labels.downward" pad.v="4"/>
							<cell rows="13">
								<vsep pad.h="2" bg.color="bg" hreduce="true"/>
							</cell>
							<void/>
							<void/>
							<cell rows="13">
								<vsep pad.h="2" bg.color="bg" hreduce="true"/>
							</cell>
							<label text="labels.compression_curve" pad.v="4"/>

							<!-- Row 2 horizontal separators -->
							<hsep pad.v="2" bg.color="bg" vreduce="true"/>
							<hsep pad.v="2" bg.color="bg" vreduce="true"/>
							<hsep pad.v="2" bg.color="bg" vreduce="true"/>
							<hsep pad.v="2" bg.color="bg" vreduce="true"/>
							<cell cols="2" bg.color="bg">
								<hsep pad.v="2" bg.color="bg" vreduce="true"/>
							</cell>
							<hsep pad.v="2" bg.color="bg" vreduce="true"/>

							<!-- rows 20 cols 7 -->
							<ui:for id="i" first="1" count="4">
								<ui:set id="ch" value="_${:i}${(:c ieq 0) ? 'm' : 's'}"/>
								<!-- rows 5 cols 7 -->

								<cell rows="2">
									<vbox>
										<ui:if test=":i ieq 1">
											<label text="labels.bands.low" pad.v="4"/>
										</ui:if>
										<ui:if test=":i ieq 2">
											<label text="labels.bands.mid" pad.v="4"/>
										</ui:if>
										<ui:if test=":i ieq 3">
											<label text="labels.bands.hi" pad.v="4"/>
										</ui:if>
										<ui:if test=":i ieq 4">
											<label text="labels.bands.hi_e" pad.v="4"/>
										</ui:if>
										<button id="bs${ch}" text="labels.chan.solo" height="19" hfill="true" pad.h="6" ui:inject="Button_green_8"/>
										<button id="bm${ch}" text="labels.chan.mute" height="19" hfill="true" pad.h="6" ui:inject="Button_red_8" pad.t="4"/>
										<button id="be${ch}" text="labels.chan.on" height="19" hfill="true" pad.h="6" ui:inject="Button_yellow_8" pad.t="4"/>
										<ui:if test=":i ilt 4">
											<void visibility="${:i}" pad.v="4"/>
										</ui:if>
										<ui:if test=":i ige 4">
											<button id="ebe" text="labels.active" height="19" hfill="true" pad.h="6" ui:inject="Button_cyan_8" pad.v="4"/>
										</ui:if>
									</vbox>
								</cell>

								<hbox hfill="false" pad.h="6">
									<ui:if test=":i ieq 1"><void/></ui:if>
									<ui:if test=":i ieq 2">
										<knob id="sf1" scolor.hue="0"/>
										<vbox vfill="false" pad.l="4">
											<label text="labels.frequency"/>
											<value id="sf1" width.min="50" sline="true" halign="0"/>
										</vbox>
									</ui:if>
									<ui:if test=":i ieq 3">
										<knob id="sf2" scolor.hue="0.25"/>
										<vbox vfill="false" pad.l="4">
											<label text="labels.frequency"/>
											<value id="sf2" width.min="50" sline="true" halign="0"/>
										</vbox>
									</ui:if>
									<ui:if test=":i ieq 4">
										<knob id="sf3" scolor.hue="0.5"/>
										<vbox vfill="false" pad.l="4">
											<label text="labels.frequency"/>
											<value id="sf3" width.min="50" sline="true" halign="0"/>
										</vbox>
									</ui:if>
								</hbox>

								<hbox hfill="false" pad.h="6">
									<knob id="tu${ch}" scolor="threshold"/>
									<vbox vfill="false" pad.l="4">
										<label text="labels.threshold"/>
										<value id="tu${ch}" width.min="50" sline="true" halign="0"/>
									</vbox>
								</hbox>
								<hbox hfill="false" pad.h="6">
									<knob id="td${ch}" scolor="threshold"/>
									<vbox vfill="false" pad.l="4">
										<label text="labels.threshold"/>
										<value id="td${ch}" width.min="50" sline="true" halign="0"/>
									</vbox>
								</hbox>
								<hbox hfill="false" pad.h="6">
									<knob id="ta${ch}" scolor="attack"/>
									<vbox vfill="false" pad.l="4">
										<label text="labels.attack"/>
										<value id="ta${ch}" width.min="50" sline="true" halign="0"/>
									</vbox>
								</hbox>
								<hbox hfill="false" pad.h="6">
									<knob id="mk${ch}"/>
									<vbox vfill="false" pad.l="4">
										<label text="labels.makeup"/>
										<value id="mk${ch}" width.min="50" sline="true" halign="0"/>
									</vbox>
								</hbox>

								<cell rows="2">
									<grid rows="2" cols="2">
										<cell rows="2">
